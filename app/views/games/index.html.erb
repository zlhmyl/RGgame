<p id="notice"><%= notice %></p>

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Info</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.name %></td>
        <td><%= game.type %></td>
        <td><%= game.info %></td>
        <td><%= game.score %></td>

        <td><%= link_to '桌游详情', game %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>



